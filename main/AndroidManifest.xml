<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mk.bankapptest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogInActivity" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".AccountSelectionActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".ViewSpendingActivity" />
        <activity android:name=".ViewTransactionsActivity" />
        <activity android:name=".TransferActivity" />
        <activity android:name=".WithdrawDepositActivity"></activity>
    </application>

</manifest>